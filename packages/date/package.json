{
  "name": "@reskit/date",
  "version": "1.3.1",
  "description": "A kit of date extraction",
  "main": "dist/reskit-date.js",
  "module": "dist/reskit-date.esm.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm run build-bundle-dev -w",
    "build": "tsx scripts/build.ts",
    "build-dev": "tsx scripts/build.ts --dev",
    "test": "tsx __test__/index.ts",
    "format": "prettier --write --parser typescript src/**/*.ts index.ts",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "(src|test)/**/*.ts": [
      "prettier --write"
    ]
  },
  "author": "simu",
  "keywords": [
    "extract date",
    "extract time",
    "extract date-time",
    "reskit",
    "date"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simo-an/reskit.git"
  },
  "homepage": "https://github.com/simo-an/reskit/tree/main/packages/date",
  "bugs": {
    "url": "https://github.com/simo-an/reskit/issues"
  },
  "dependencies": {
    "@reskit/number": "workspace:*",
    "@reskit/shared": "workspace:*"
  },
  "devDependencies": {}
}