    "reskit",
    "extract"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simo-an/reskit.git"
  },
  "homepage": "https://github.com/simo-an/reskit/tree/main/packages/color",
  "bugs": {
    "url": "https://github.com/simo-an/reskit/issues"
  },
