{
  "name": "@reskit/color",
  "version": "1.3.0",
  "description": "A kit of color extraction",
  "main": "dist/reskit-color.js",
  "module": "dist/reskit-color.esm.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm run build-bundle-dev -w",
